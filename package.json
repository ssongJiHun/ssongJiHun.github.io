{
  "name": "blogs",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby clean && gatsby develop",
    "start": "gatsby clean && gatsby develop",
    "build": "gatsby clean && env-cmd -f .env.production gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "env-cmd -f .env.production gatsby clean && gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "env-cmd": "^10.1.0",
    "fontsource-noto-sans-kr": "^4.0.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-emotion": "^8.10.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-remark-autolink-headers": "^6.10.0",
    "gatsby-remark-copy-linked-files": "^6.10.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "^7.10.0",
    "gatsby-remark-smartypants": "^6.10.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "gh-pages": "^5.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "yarn@3.6.0"
}
